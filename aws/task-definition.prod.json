{
  "family": "blinkhash-prod-website",
  "executionRoleArn": "arn:aws:iam::348473643547:role/Blinkhash-ECS",
  "containerDefinitions": [
    {
      "name": "blinkhash-prod-website",
      "image": "348473643547.dkr.ecr.us-east-1.amazonaws.com/blinkhash-website:latest",
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "healthCheck": {
        "retries": 3,
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost/health || exit 1"
        ],
        "timeout": 5,
        "interval": 30,
        "startPeriod": 60
      },
      "cpu": 512,
      "memory": 1024,
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "blinkhash-prod-website",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs-prod"
        }
      }
    }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048"
}
